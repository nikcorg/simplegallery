RewriteEngine On
RewriteBase /

RewriteCond	%{REQUEST_URI}		rss
RewriteRule	(.*)			index.php?feed=1 [L]

RewriteCond 	%{REQUEST_URI} 		gallery
RewriteRule 	gallery/([^/]*)		index.php?galleryID=$1 [QSA,L]

RewriteCond     %{REQUEST_URI}      /index/
RewriteRule     .                   index.php?index=1   [QSA,L]
